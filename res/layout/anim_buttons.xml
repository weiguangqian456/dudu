        android:background="@drawable/vs_dia_keyboard_09" />

    <ImageButton
        android:id="@+id/DigitHelperButton"
        android:layout_width="fill_parent"
        android:layout_height="63.5dp"
        android:contentDescription="@string/app_name"
        android:gravity="center"
        android:background="@drawable/vs_dia_keyboard_paste" />

    <ImageButton
        android:id="@+id/DigitZeroButton"
        android:layout_width="fill_parent"
        android:layout_height="63.5dp"
        android:contentDescription="@string/app_name"
        android:gravity="center"
        android:background="@drawable/vs_dia_keyboard_00" />

    <ImageButton
        android:id="@+id/DigitDeleteButton"
        android:layout_width="fill_parent"
        android:layout_height="63.5dp"
        android:contentDescription="@string/app_name"
        android:gravity="center"
        android:background="@drawable/vs_dia_keyboard_del" />

    <Button
        android:id="@+id/btn_menu"
        android:layout_width="105dp"
        android:layout_height="63.5dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:background="@drawable/vs_dia_keyboard_paste" />

</RelativeLayout>