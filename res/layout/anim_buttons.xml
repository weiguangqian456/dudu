<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#FFF" >

    <ImageButton
        android:id="@+id/DigitOneButton"
        android:layout_width="fill_parent"
        android:layout_height="63.5dp"
        android:contentDescription="@string/app_name"
        android:gravity="center"
        android:background="@drawable/vs_dia_keyboard_01" />

    <ImageButton
        android:id="@+id/DigitTwoButton"
        android:layout_width="fill_parent"
        android:layout_height="63.5dp"
        android:contentDescription="@string/app_name"
        android:gravity="center"
        android:background="@drawable/vs_dia_keyboard_02" />

    <ImageButton
        android:id="@+id/DigitThreeButton"
        android:layout_width="fill_parent"
        android:layout_height="63.5dp"
        android:contentDescription="@string/app_name"
        android:gravity="center"
        android:background="@drawable/vs_dia_keyboard_03" />

    <ImageButton
        android:id="@+id/DigitFourButton"
        android:layout_width="fill_parent"
        android:layout_height="63.5dp"
        android:contentDescription="@string/app_name"
        android:gravity="center"
