                            android:layout_marginRight="3dp"
                            android:layout_marginTop="3dp"
                            android:background="@drawable/st_btn_numb_bg"
                            android:gravity="center"
                            android:textColor="@color/White"
                            android:textSize="9sp"
                            android:visibility="gone" />

                    </RelativeLayout>

                    <!--我的界面-->


                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="@dimen/w_46_dip"
                        android:layout_weight="1"
                        android:gravity="center">


                        <LinearLayout
                            android:id="@+id/vs_me_layout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/vs_me_imagview"
                                android:layout_width="@dimen/w_21_dip"
                                android:layout_height="@dimen/w_21_dip"
                                android:background="@drawable/icon_home_mine_n" />

                            <TextView
                                android:id="@+id/vs_me_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/w_4_dip"
                                android:gravity="center"
                                android:text="@string/tab_myself"
                                android:textColor="@color/text_gray"
                                android:textSize="@dimen/w_10_dip" />

                        </LinearLayout>

                        <ImageView
                            android:id="@+id/vs_me_layout_red"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignRight="@id/vs_me_layout"
                            android:layout_alignTop="@id/vs_me_layout"
                            android:layout_marginRight="3dp"
                            android:layout_marginTop="3dp"
                            android:background="@drawable/vs_red_dot"
                            android:visibility="gone" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignRight="@id/vs_me_layout"
                            android:layout_alignTop="@id/vs_me_layout"
                            android:layout_marginRight="3dp"
                            android:layout_marginTop="3dp"
                            android:background="@drawable/st_btn_numb_bg"
                            android:gravity="center"
                            android:textColor="@color/White"
                            android:textSize="9sp"
                            android:visibility="gone" />

                    </RelativeLayout>


                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:visibility="gone">


                        <LinearLayout
                            android:id="@+id/vs_service_layout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/vs_service_imagview"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/ic_service" />

                            <TextView
                                android:id="@+id/vs_service_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="@string/tab_service"
                                android:textColor="@color/text_gray"
                                android:textSize="11sp" />

                        </LinearLayout>

                        <ImageView
                            android:id="@+id/vs_service_layout_red"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignRight="@id/vs_service_layout"
                            android:layout_alignTop="@id/vs_service_layout"
                            android:layout_marginRight="3dp"
                            android:layout_marginTop="3dp"
                            android:background="@drawable/vs_red_dot"
                            android:visibility="gone" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignRight="@id/vs_service_layout"
                            android:layout_alignTop="@id/vs_service_layout"
                            android:layout_marginRight="3dp"
                            android:layout_marginTop="3dp"
                            android:background="@drawable/st_btn_numb_bg"
                            android:gravity="center"
                            android:textColor="@color/White"
                            android:textSize="9sp"
                            android:visibility="gone" />

                    </RelativeLayout>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:id="@+id/vs_keyborad_layout"
            android:layout_width="@dimen/w_37_dip"
            android:layout_height="@dimen/w_37_dip"
            android:layout_alignParentBottom="true"
            android:layout_centerInParent="true"
            android:clipChildren="false"
            android:gravity="center"
            android:layout_marginBottom="@dimen/w_19_dip"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/vs_keyborad_imagview"
                android:layout_width="@dimen/w_36_dip"
                android:layout_height="@dimen/w_36_dip"
                android:layout_gravity="center"
                android:background="@drawable/dudujiayou2x" />

        </LinearLayout>


    </RelativeLayout>


