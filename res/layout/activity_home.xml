                    <!--我的订单-->

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="@dimen/w_46_dip"
                        android:layout_weight="1"
                        android:gravity="center">

                        <LinearLayout
                            android:id="@+id/ll_classify"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/iv_classify"
                                android:layout_width="@dimen/w_19_dip"
                                android:layout_height="@dimen/w_21_dip"
                                android:background="@drawable/myorder2x" />

                            <TextView
                                android:id="@+id/tv_classify"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/w_4_dip"
                                android:gravity="center"
                                android:text="@string/myorder"
                                android:textColor="@color/text_gray"
                                android:textSize="@dimen/w_10_dip" />

                        </LinearLayout>
                    </RelativeLayout>


                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:visibility="gone">


                        <LinearLayout
                            android:id="@+id/vs_mms_layout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/vs_mms_imagview"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/ic_mms_normal" />

                            <TextView
                                android:id="@+id/vs_mms_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="@string/tab_mms"
                                android:textColor="@color/text_gray"
                                android:textSize="11sp" />

                        </LinearLayout>

                        <ImageView
                            android:id="@+id/vs_mms_imagview_red"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignRight="@id/vs_mms_layout"
                            android:layout_alignTop="@id/vs_mms_layout"
                            android:layout_marginRight="3dp"
                            android:layout_marginTop="3dp"
                            android:background="@drawable/vs_red_dot"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/vs_mms_unread"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignRight="@id/vs_mms_layout"
