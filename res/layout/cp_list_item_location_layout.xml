<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center_vertical"
    android:padding="16dp">

    <FrameLayout
        android:id="@+id/cp_list_item_location_layout"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/w_50_dip"
        android:clickable="true"
        android:focusable="true"
        tools:ignore="UselessParent">

        <TextView
            android:id="@+id/cp_list_item_location"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_hotcity"
            android:paddingBottom="@dimen/w_10_dip"
            android:paddingTop="@dimen/w_10_dip"
            android:text="shenzhen"
            android:gravity="center"
            android:textColor="@color/public_color_666666"
            android:textSize="@dimen/w_10_dip"
            tools:ignore="RtlCompat" />
    </FrameLayout>
</LinearLayout>